FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.02;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.particles;
        inletValue      $internalField;
        value           $internalField;
    }
    top
    {
        type            slip;
    }
    land
    {
        type            zeroGradient;
    }
    
    plane
    {
        type            zeroGradient;
    }
    
    frontAndBack
    {
        type            empty;
    }
}


